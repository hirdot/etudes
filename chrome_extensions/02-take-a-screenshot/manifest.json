{
  "name": "Example 2: Take a Screenshot",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is an example to take a screenshot",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Take a screen shot!"
  },
  "permissions": [
    "<all_urls>"
  ]
}
